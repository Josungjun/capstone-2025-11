<launch>
    <arg name="bag_file" default="$(find test)/bags/plz.bag"/>
    <arg name="rate" default="1.0"/>
    
    <node pkg="rosbag" type="play" name="rosbag_play" args="$(arg bag_file)">
        <remap from="/cam0/image_raw" to="/usb_cam/image_raw"/>
        <remap from="/imu0" to="/handsfree/imu"/>
    </node>
</launch>
