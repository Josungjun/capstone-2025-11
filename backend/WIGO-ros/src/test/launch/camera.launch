<launch>
    <!-- ROS time을 사용하도록 설정 -->
    <param name="/use_sim_time" value="true" />

    <!-- rosbag 재생 -->
    <node pkg="rosbag" type="play" name="rosbag_play"
          args="--clock $(find test)/bags/plz.bag --start 40">
          
          <remap from='/cam0/image_raw' to='/usb_cam/image_raw' />
          <remap from='/imu0' to='/handsfree/imu' />

    </node>       <!-- start 40 -->
</launch>

